@model InsuranceRiskAssessment.Web.Models.ViewModels.MovableProprtiesViewModels.SeaTransport.SeaTransportDetailsViewModel

@{
    ViewData["Title"] = "Insurance Risk Assesment - Изтрий резултат";
}

<div class="delete-container">
    <div class="delete-box">
        <a asp-action="Index"><i data-feather="arrow-left" class="back-icon-delete"></i></a>
        <p class='delete-header'>Сигурни ли сте, че искате да изтриете този резултат?</p>
        <label asp-for="Name"></label>
        <span class="delete-span">@Html.DisplayFor(model => model.Name)</span><br />
        <label asp-for="ResultValue"></label>
        <span class="delete-span">@Html.DisplayFor(model => model.ResultValue)</span>
        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Изтрий" class="delete-btn" />
        </form>
    </div>
</div>
